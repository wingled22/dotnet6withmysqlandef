<template id="card-container">
    <div class="card m-2" style="width: 300px;">
        <div class="card-body">
            <h5 class="card-title">Card title</h5>
            <h6 class="card-subtitle mb-2 text-muted">Card subtitle</h6>
            <p class="card-text card-stocks">Unit: </p>
            <p class="card-text card-units">Stock:</p>
            <p class="card-text card-status">Status: </p>

            <a class="card-link btn btn-success edit-prod" data-bs-toggle="tooltip" data-bs-placement="top" title="Tooltip on top">
                <i class="fa-solid fa-pen"></i>
            </a>
            <a class="btn btn-warning">
                <i class="fa-solid fa-circle-info" style="color: #ffffff;"></i>
            </a>

            <a class="btn btn-warning">
               <i class="fa-solid fa-cubes" style="color: #ffffff;"></i>
            </a>
            
            <a class="btn btn-warning">
                <i class="fa-solid fa-clock-rotate-left" style="color: #ffffff;"></i>
            </a>
            <a class="btn btn-danger">
                <i class="fa-solid fa-trash"></i>
            </a>
        </div>
    </div>
</template>
